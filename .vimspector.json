{
  "configurations": {
    "run": {
      "adapter": "debugpy",
      "breakpoints": {
        "exception": {
          "all": "N",
          "uncaught": "N"
        }
      },
      "configuration": {
        "request": "launch",
        "protocol": "auto",
        "stopOnEntry": true,
        "console": "integratedTerminal",
        "module": "IPython",
        "args": [
          "-i",
          "${workspaceRoot}/test_pymol.py"
        ],
        "cwd": "${workspaceRoot}"
      }
    }
  }
}
