            }
        },
        {
            "name": "Pymol37-Winbugs",
            "type": "python",
            "request": "launch",
            "args": ["-i", "test_pymol.py"],
            "module": "IPython",
            "console": "integratedTerminal",
            "justMyCode": false,
            "python": "${workspaceFolder}\\.virtualenv\\Scripts\\python.exe",
            "env": {
