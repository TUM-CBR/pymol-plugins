            "module": "IPython",
            "console": "integratedTerminal",
            "justMyCode": false,
            "python": "${workspaceFolder}/.virtualenv/bin/python",
            "env": {
                "QTHREAD_DEBUGGING": "True",
                "PATH": "/home/ernesto/code/dssp/dssp/dist/bin:/home/ernesto/code/ACPSICOV/src/:/home/ernesto/code/cbr-tools-extra/dist/cbrtools/:/home/ernesto/code/ProteinMPNN/:$PATH",
